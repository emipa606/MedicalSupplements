<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- Industrial -->
  <ThingDef ParentName="ResourceBase">
    <defName>MSMedicalMatItem</defName>
    <label>Rolled up medical mat</label>
    <description>A rolled up portable mat that can be used for medical purposes in the field or as a useful caravan item.</description>
    <graphicData>
      <texPath>Things/Item/Resource/MSMedicalMatItem</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>Standard_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>5</stackLimit>
    <statBases>
      <MaxHitPoints>80</MaxHitPoints>
      <MarketValue>262</MarketValue>
      <Mass>2.0</Mass>
      <Flammability>0.75</Flammability>
      <DeteriorationRate>1.0</DeteriorationRate>
    </statBases>
    <techLevel>Industrial</techLevel>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
  </ThingDef>
