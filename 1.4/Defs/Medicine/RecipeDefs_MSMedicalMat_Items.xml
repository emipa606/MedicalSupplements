<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- Industrial -->
  <RecipeDef>
    <defName>MakeMsMedicalMat</defName>
    <label>Make Medical Mat</label>
    <description>Make a medical mat.</description>
    <jobString>Making a medical mat.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Tailor</effectWorking>
    <soundWorking>Recipe_Tailor</soundWorking>
    <workAmount>500</workAmount>
    <targetCountAdjustment>1</targetCountAdjustment>
    <allowMixingIngredients>true</allowMixingIngredients>
    <workSkill>Crafting</workSkill>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Leathers</li>
          </categories>
        </filter>
        <count>200</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Neutroamine</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Leathers</li>
      </categories>
      <thingDefs>
        <li>Neutroamine</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <MSMedicalMatItem>1</MSMedicalMatItem>
    </products>
    <recipeUsers>
      <li>ElectricTailoringBench</li>
      <li>HandTailoringBench</li>
    </recipeUsers>
    <skillRequirements>
      <Crafting>7</Crafting>
    </skillRequirements>
    <researchPrerequisite>MSMedicalMat</researchPrerequisite>
  </RecipeDef>
