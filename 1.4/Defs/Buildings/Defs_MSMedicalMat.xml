<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- Medical Mats -->
  <!-- Industrial -->
  <ThingDef ParentName="BedWithQualityBase">
    <defName>MSMedicalMat</defName>
    <label>Medical Mat</label>
    <description>A portable mat that can be used for medical purposes in the field.</description>
    <graphicData>
      <texPath>Things/Building/Furniture/MSMedicalMat/MSMedicalMat</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(1,2)</drawSize>
    </graphicData>
    <minifiedDef>MinifiedThing</minifiedDef>
    <castEdgeShadows>True</castEdgeShadows>
    <statBases>
      <MaxHitPoints>80</MaxHitPoints>
      <Flammability>0.75</Flammability>
      <Mass>2.0</Mass>
      <BedRestEffectiveness>0.90</BedRestEffectiveness>
      <ImmunityGainSpeedFactor>1.1</ImmunityGainSpeedFactor>
      <MedicalTendQualityOffset>0.15</MedicalTendQualityOffset>
      <SurgerySuccessChanceFactor>1.05</SurgerySuccessChanceFactor>
      <Comfort>0.78</Comfort>
      <WorkToBuild>30</WorkToBuild>
      <MarketValue>262</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_AffectedByFacilities">
        <linkableFacilities>
          <li>MS_DStand</li>
          <li>VitalsMonitor</li>
          <li>Dresser</li>
          <li>EndTable</li>
        </linkableFacilities>
      </li>
    </comps>
    <size>(1,2)</size>
    <costList>
      <MSMedicalMatItem>1</MSMedicalMatItem>
    </costList>
    <building>
      <bed_showSleeperBody>false</bed_showSleeperBody>
      <bed_healPerDay>9</bed_healPerDay>
      <bed_caravansCanUse>true</bed_caravansCanUse>
      <bed_defaultMedical>true</bed_defaultMedical>
    </building>
    <techLevel>Industrial</techLevel>
    <pathCost>5</pathCost>
    <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
    <resourcesFractionWhenDeconstructed>1.0</resourcesFractionWhenDeconstructed>
    <passability>Standable</passability>
    <designationHotKey>Misc2</designationHotKey>
    <researchPrerequisites>
      <li>MSMedicalMat</li>
    </researchPrerequisites>
    <thingCategories Inherit="false">
      <li>BuildingsMisc</li>
    </thingCategories>
    <designationCategory>Misc</designationCategory>
  </ThingDef>
